<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/SelectorFragmentContainer"
        android:name="com.example.cis436_project3.ui.main.SelectorFragment"
        android:layout_width="394dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toTopOf="@+id/SpinnerFragmentContainer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.421" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/ScrollViewFragmentContainer"
        android:name="com.example.cis436_project3.ui.main.ScrollViewFragment"
        android:layout_width="405dp"
        android:layout_height="585dp"
        android:layout_marginTop="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SpinnerFragmentContainer"
        app:layout_constraintVertical_bias="0.183" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/SpinnerFragmentContainer"
        android:name="com.example.cis436_project3.ui.main.SpinnerFragment"
        android:layout_width="385dp"
        android:layout_height="78dp"
        android:layout_marginTop="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>